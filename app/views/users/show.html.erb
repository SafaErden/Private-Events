<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @current_user.name %>
</p>

<h2>Created Events</h2>
<% unless @current_user.events.nil? %>
  <% @current_user.events.each do |event| %>
    <p>Event: <%= event.title %></p>
    <p>Date: <%= event.date %></p>
  <% end %>
<% end %>

<h2>Previous Events</h2>
<% unless @previous_events.nil? %>
  <% @previous_events.each do |attended_event| %>
    <div>
      <p>Event: <%= attended_event.try(:title) %></p>
      <p>Date: <%= attended_event.try(:date) %></p>
    </div>
  <% end %>
<% end %>

<h2>Upcoming Events</h2>
<% unless @upcoming_events.nil? %>
  <% @upcoming_events.each do |attended_event| %>
    <div>
      <p>Event: <%= attended_event.try(:title) %></p>
      <p>Date: <%= attended_event.try(:date) %></p>
    </div>
  <% end %>
<% end %>

<%#= link_to 'Back', users_path %>
